<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="2" android:versionName="0.3" package="be.philipluyckx.geocaching">

    <uses-sdk android:minSdkVersion="15" android:targetSdkVersion="16"/>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="be.philipluyckx.geocaching.GeocachingApplication" android:theme="@style/AppTheme">

        <activity android:label="@string/app_name" android:name="be.philipluyckx.geocaching.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
